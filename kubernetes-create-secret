create secrets

kubectl create secret generic my-secret --from-literal=DB_HOST=db-host --from-literal=DB_MODE=PRODUCTION --from-literal=DB_NAME=db-name --from-literal=DB_USER_NAME=db-user-name --from-literal=DB_PASSWORD=db-password --from-literal=JWT_SECRET=jwt-secret --from-literal=TWILIO_ACCOUNT_SID=twilio-account-sid --from-literal=TWILIO_AUTH_TOKEN=twilio-auth-token --from-literal=TWILIO_FROM_NUMBER="twilio-from-number" --from-literal=CISS_SMS_TOKEN="ciss-sms-token" --from-literal=PHOTO_BASE_URL="photo-base-url" --from-literal=SOCKET_IP="socket-ip"
